            <term_definitions code="at0000">
              <items id="text">Alcohol Use Summary</items>
              <items id="description">Summary or persisting information about alcohol use or consumption.</items>
            </term_definitions>
            <term_definitions code="at0001">
              <items id="text">Tree</items>
              <items id="description">@ internal @</items>
            </term_definitions>
            <term_definitions code="at0002">
              <items id="text">Status</items>
              <items id="description">Statement about the individual's overall pattern of usage or consumption of alcohol.</items>
            </term_definitions>
            <term_definitions code="at0003">
              <items id="text">Current drinker</items>
              <items id="description">Individual is a current consumer of alcohol.</items>
            </term_definitions>
            <term_definitions code="at0004">
              <items id="text">Ex-drinker</items>
              <items id="description">Individual is a former or ex-consumer of alcohol.</items>
            </term_definitions>
            <term_definitions code="at0006">
              <items id="text">Lifetime non-drinker</items>
              <items id="description">Individual has never consumed alcohol.</items>
            </term_definitions>
            <term_definitions code="at0015">
              <items id="text">Date Commenced</items>
              <items id="description">Date that any consumption of alcohol commenced.</items>
              <items id="comment">In most situations it is likely that only a partial date will be recorded, for example, only the year of commencement.</items>
            </term_definitions>
            <term_definitions code="at0016">
              <items id="text">Date Ceased</items>
              <items id="description">Date that all consumption of alcohol ceased.</items>
              <items id="comment">Can be a partial date, for example, only a year.</items>
            </term_definitions>
            <term_definitions code="at0019">
              <items id="text">Comment</items>
              <items id="description">Additional narrative about the alcohol use or consumption pattern not captured in other fields.</items>
            </term_definitions>
            <term_definitions code="at0021">
              <items id="text">Tree</items>
              <items id="description">@ internal @</items>
            </term_definitions>
            <term_definitions code="at0022">
              <items id="text">Date Last Updated</items>
              <items id="description">The date this alcohol consumption summary was last updated.</items>
            </term_definitions>
            <term_definitions code="at0023">
              <items id="text">Cessation Attempts</items>
              <items id="description">Details about attempts to cease use of alcohol.</items>
            </term_definitions>
            <term_definitions code="at0024">
              <items id="text">Typical Alcohol Consumption</items>
              <items id="description">Estimate of typical alcohol consumption, in number of standard drinks/units per day - either as a whole number, a range, or as a term, normally coded. Definitions of standard units/drinks vary considerably.</items>
              <items id="comment">This data element allows a rough indication of alcohol consumption to be recorded, for example 5-10 per day. The period of time is not specified. If exact consumption at specific points in time or averages/maximums over specified intervals of time are required, use the OBSERVATION.alcohol_use archetype.</items>
            </term_definitions>
            <term_definitions code="at0025">
              <items id="text">Form</items>
              <items id="description">The form or type of alcohol consumed.</items>
              <items id="comment">>> 53527002 | alcoholic beverage |</items>
            </term_definitions>
            <term_definitions code="at0026">
              <items id="text">Binge Drinking Pattern</items>
              <items id="description">The subject's pattern of heavy, episodic drinking.</items>
            </term_definitions>
            <term_definitions code="at0027">
              <items id="text">Pattern of Use</items>
              <items id="description">The typical pattern of the subject's use of alcohol.</items>
            </term_definitions>
            <term_definitions code="at0028">
              <items id="text">None</items>
              <items id="description">The subject does not binge drink.</items>
            </term_definitions>
            <term_definitions code="at0029">
              <items id="text">Less than once per month</items>
              <items id="description">The subject binge drinks less than once per month.</items>
            </term_definitions>
            <term_definitions code="at0030">
              <items id="text">Monthly</items>
              <items id="description">The subject binge drinks on a monthly basis.</items>
            </term_definitions>
            <term_definitions code="at0031">
              <items id="text">Weekly</items>
              <items id="description">The subject binge drinks on a weekly basis.</items>
            </term_definitions>
            <term_definitions code="at0032">
              <items id="text">Daily</items>
              <items id="description">The subject binge drinks on a daily or almost daily basis.</items>
            </term_definitions>
            <term_definitions code="at0033">
              <items id="text">Standard Drink Definition</items>
              <items id="description">Amount of alcohol defining a standard drink or unit.</items>
            </term_definitions>
