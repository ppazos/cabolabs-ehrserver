                <term_definitions code="at0004">
                  <items id="text">Heart failure classification</items>
                  <items id="description">The functional classification of heart failure based on patient symptoms.</items>
                </term_definitions>
                <term_definitions code="at0005">
                  <items id="text">Class I</items>
                  <items id="description">No limitation of physical activity. Ordinary physical activity does not cause undue fatigue, palpitation, or dyspnoea.</items>
                </term_definitions>
                <term_definitions code="at0006">
                  <items id="text">Class II</items>
                  <items id="description">Slight limitation of physical activity. Comfortable at rest, but ordinary physical activity results in fatigue, palpitation, or dyspnoea.</items>
                </term_definitions>
                <term_definitions code="at0007">
                  <items id="text">Class III</items>
                  <items id="description">Marked limitation of physical activity. Comfortable at rest, but less than ordinary activity results in fatigue, palpitation, or dyspnoea.</items>
                </term_definitions>
                <term_definitions code="at0008">
                  <items id="text">Class IV</items>
                  <items id="description">Unable to carry on any physical activity without discomfort. Symptoms at rest. If any physical activity is undertaken, discomfort is increased.</items>
                </term_definitions>
                <term_definitions code="at0009">
                  <items id="text">Comment</items>
                  <items id="description">Additional comment about the NYHA heart failure classification result.</items>
                </term_definitions>
